{
  "manifest_version": 2,

  "name": "StatsWebsite",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/app/icon.png",
    "default_popup": "popup.html",
    "default_title": "Magnify"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "activeTab",
    "tts",
    "http://*/"
  ]
}
